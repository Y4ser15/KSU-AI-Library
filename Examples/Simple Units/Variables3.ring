loadsyntax "ArabicSyntax.ring"
 
س =10 // س متغير من نوع (رقم/ int)
ص = "20" // ص متغير من نوع (نص/ string)
الناتج = س + ص // الناتج متغير من نوع (رقم / int )(ص سوف يتحول الى نوع (رقم / int))
  ا = "ناتج الجمع = " + الناتج //ا متغير من نوع(نص / string)( الناتج سوف يتحول الى نوع(نص / string))
اطبع ا
